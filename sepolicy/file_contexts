# Gestures
/sys/devices/virtual/touchscreen/touchscreen_dev/gesture_ctrl  u:object_r:touchpanel_sysfs:s0
/sys/devices/soc.0/78b9000.i2c/i2c-5/5-005d/keypad_enable      u:object_r:touchpanel_sysfs:s0

# Battery
/sys/devices/soc\.0/qpnp-linearcharger-[0-9a-f]+/power_supply/battery/capacity  u:object_r:sysfs_batteryinfo:s0

# Yulong interfaces
/dev/yl_alsprox_sensor                          u:object_r:sensors_device:s0
/dev/yl_params1                                 u:object_r:ylparams_device:s0

# Bluetooth
/system/bin/yl_btmac                            u:object_r:bluetooth_loader_exec:s0
/persist/.bt_nv.bin                             u:object_r:bluetooth_data_file:s0
/system/etc/init\.qcom\.bt\.sh                  u:object_r:bluetooth_loader_exec:s0
/dev/smd3                                       u:object_r:hci_attach_dev:s0

# Wi-Fi MAC Adress
/persist/.genmac                                u:object_r:wifi_data_file:s0

# Sensors
/dev/mmc3416x                                   u:object_r:sensors_device:s0
